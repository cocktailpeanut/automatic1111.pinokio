{
  "prompt": "a Maneki-neko in the middle of a desert",
  "run": [
    {
      "uri": "../index.js",
      "method": "run",
      "params": {
        "cfg_scale": 7,
        "steps": 30,
        "prompt": "{{self.prompt}}"
      },
      "queue": true
    },
    {
      "method": "goto",
      "params": {
        "index": "{{input.error ? null : 2}}",
        "input": "{{input}}"
      }
    },
    {
      "method": "set",
      "params": {
        "local": {
          "path": "../images/{{Date.now()}}.png",
          "image": "{{Buffer.from(input.images[0], 'base64')}}"
        }
      }
    },
    {
      "method": "fs.write",
      "params": {
        "path": "{{local.path}}",
        "buffer": "{{local.image}}"
      }
    },
    {
      "method": "browser",
      "params": {
        "method": "open",
        "params": ["{{local.path}}", "new", "app"]
      }
    },
    {
      "method": "goto",
      "params": {
        "index": 0
      }
    }
  ]
}
